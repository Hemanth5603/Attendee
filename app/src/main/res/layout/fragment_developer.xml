<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FCFFF9"
    tools:context=".Developer">


    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="328dp"
        android:layout_height="429dp"
        android:background="@drawable/custom_linearlayout"
        android:backgroundTint="#FFFFFF"
        android:elevation="5dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView13"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="80dp"
            android:fontFamily="@font/seogi"
            android:gravity="center"
            android:text="Hemanth Srinivas"
            android:textColor="@color/black"
            android:textSize="28dp" />


        <TextView
            android:id="@+id/textView15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="1st Year"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/seogi"
            android:gravity="center"
            android:textColor="@color/black"
            android:textSize="22dp"/>

        <TextView
            android:id="@+id/textView16"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="4dp"
            android:fontFamily="@font/seogi"
            android:gravity="center"
            android:text="Raghu Institute of Technology"
            android:textColor="@color/black"
            android:textSize="22dp"/>

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginTop="100dp"
            android:src="@drawable/developers" />

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="250dp"
        android:layout_height="120dp"
        android:src="@drawable/ritlogo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

</androidx.constraintlayout.widget.ConstraintLayout>